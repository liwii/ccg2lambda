<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="candc.xml"?>

<candc>
<ccg>
 <rule type="fa" cat="S[dcl]">
  <rule type="fa" cat="S[X]/S[X]">
   <lf start="0" span="1" word="In" lemma="in" pos="IN" chunk="I-PP" entity="O" cat="(S/S)/NP" />
   <rule type="lex" cat="NP">
    <lf start="1" span="1" word="1994" lemma="1994" pos="CD" chunk="I-NP" entity="I-DAT" cat="N" />
   </rule>
  </rule>
  <rule type="rp" cat="S[dcl]">
   <rule type="ba" cat="S[dcl]">
    <rule type="lex" cat="NP">
     <lf start="2" span="1" word="ITEL" lemma="ITEL" pos="NNP" chunk="I-NP" entity="I-DAT" cat="N" />
    </rule>
    <rule type="ba" cat="S[dcl]\NP">
     <rule type="fa" cat="S[dcl]\NP">
      <rule type="fa" cat="(S[dcl]\NP)/NP">
       <lf start="3" span="1" word="sent" lemma="send" pos="VBD" chunk="I-VP" entity="O" cat="((S[dcl]\NP)/NP)/NP" />
       <rule type="fa" cat="NP[nb]">
        <lf start="4" span="1" word="a" lemma="a" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
        <lf start="5" span="1" word="progress" lemma="progress" pos="NN" chunk="I-NP" entity="O" cat="N" />
       </rule>
      </rule>
      <rule type="lex" cat="NP">
       <lf start="6" span="1" word="report" lemma="report" pos="NN" chunk="I-NP" entity="O" cat="N" />
      </rule>
     </rule>
     <rule type="fa" cat="(S[X]\NP)\(S[X]\NP)">
      <lf start="7" span="1" word="every" lemma="every" pos="DT" chunk="B-NP" entity="I-DAT" cat="((S\NP)\(S\NP))/N" />
      <lf start="8" span="1" word="month" lemma="month" pos="NN" chunk="I-NP" entity="I-DAT" cat="N" />
     </rule>
    </rule>
   </rule>
   <lf start="9" span="1" word="." lemma="." pos="." chunk="O" entity="O" cat="." />
  </rule>
 </rule>
</ccg>
<deps>
<![CDATA[
]]></deps>
<ccg>
 <rule type="rp" cat="S[dcl]">
  <rule type="ba" cat="S[dcl]">
   <rule type="lex" cat="NP">
    <lf start="0" span="1" word="ITEL" lemma="ITEL" pos="NNP" chunk="I-NP" entity="I-DAT" cat="N" />
   </rule>
   <rule type="ba" cat="S[dcl]\NP">
    <rule type="fa" cat="S[dcl]\NP">
     <rule type="fa" cat="(S[dcl]\NP)/NP">
      <lf start="1" span="1" word="sent" lemma="send" pos="VBD" chunk="I-VP" entity="O" cat="((S[dcl]\NP)/NP)/NP" />
      <rule type="fa" cat="NP[nb]">
       <lf start="2" span="1" word="a" lemma="a" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
       <lf start="3" span="1" word="progress" lemma="progress" pos="NN" chunk="I-NP" entity="O" cat="N" />
      </rule>
     </rule>
     <rule type="lex" cat="NP">
      <lf start="4" span="1" word="report" lemma="report" pos="NN" chunk="I-NP" entity="O" cat="N" />
     </rule>
    </rule>
    <rule type="fa" cat="(S[X]\NP)\(S[X]\NP)">
     <lf start="5" span="1" word="in" lemma="in" pos="IN" chunk="I-PP" entity="O" cat="((S\NP)\(S\NP))/NP" />
     <rule type="lex" cat="NP">
      <rule type="ba" cat="N">
       <lf start="6" span="1" word="July" lemma="July" pos="NNP" chunk="I-NP" entity="I-DAT" cat="N" />
       <lf start="7" span="1" word="1994" lemma="1994" pos="CD" chunk="I-NP" entity="I-DAT" cat="N\N" />
      </rule>
     </rule>
    </rule>
   </rule>
  </rule>
  <lf start="8" span="1" word="." lemma="." pos="." chunk="O" entity="O" cat="." />
 </rule>
</ccg>
<deps>
<![CDATA[
]]></deps>
</candc>
