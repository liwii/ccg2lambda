<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="candc.xml"?>

<candc>
<ccg>
 <rule type="fa" cat="S[dcl]">
  <rule type="fa" cat="S[X]/S[X]">
   <lf start="0" span="1" word="In" lemma="in" pos="IN" chunk="I-PP" entity="O" cat="(S/S)/NP" />
   <rule type="lex" cat="NP">
    <lf start="1" span="1" word="March" lemma="March" pos="NNP" chunk="I-NP" entity="I-DAT" cat="N" />
   </rule>
  </rule>
  <rule type="rp" cat="S[dcl]">
   <rule type="ba" cat="S[dcl]">
    <rule type="lex" cat="NP">
     <rule type="fa" cat="N">
      <lf start="2" span="1" word="1993" lemma="1993" pos="CD" chunk="I-NP" entity="I-DAT" cat="N/N" />
      <lf start="3" span="1" word="APCOM" lemma="APCOM" pos="NNP" chunk="I-NP" entity="I-ORG" cat="N" />
     </rule>
    </rule>
    <rule type="fa" cat="S[dcl]\NP">
     <lf start="4" span="1" word="founded" lemma="found" pos="VBD" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/NP" />
     <rule type="lex" cat="NP">
      <lf start="5" span="1" word="ITEL" lemma="ITEL" pos="NNP" chunk="I-NP" entity="I-ORG" cat="N" />
     </rule>
    </rule>
   </rule>
   <lf start="6" span="1" word="." lemma="." pos="." chunk="O" entity="O" cat="." />
  </rule>
 </rule>
</ccg>
<deps>
<![CDATA[
]]></deps>
<ccg>
 <rule type="rp" cat="S[dcl]">
  <rule type="ba" cat="S[dcl]">
   <rule type="lex" cat="NP">
    <lf start="0" span="1" word="ITEL" lemma="ITEL" pos="NNP" chunk="I-NP" entity="I-ORG" cat="N" />
   </rule>
   <rule type="ba" cat="S[dcl]\NP">
    <lf start="1" span="1" word="existed" lemma="exist" pos="VBD" chunk="I-VP" entity="O" cat="S[dcl]\NP" />
    <rule type="fa" cat="(S[X]\NP)\(S[X]\NP)">
     <lf start="2" span="1" word="in" lemma="in" pos="IN" chunk="I-PP" entity="O" cat="((S\NP)\(S\NP))/NP" />
     <rule type="lex" cat="NP">
      <lf start="3" span="1" word="1992" lemma="1992" pos="CD" chunk="I-NP" entity="I-DAT" cat="N" />
     </rule>
    </rule>
   </rule>
  </rule>
  <lf start="4" span="1" word="." lemma="." pos="." chunk="O" entity="O" cat="." />
 </rule>
</ccg>
<deps>
<![CDATA[
]]></deps>
</candc>
