<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="candc.xml"?>

<candc>
<ccg>
 <rule type="rp" cat="S[dcl]">
  <rule type="ba" cat="S[dcl]">
   <rule type="lex" cat="NP">
    <rule type="fa" cat="N">
     <rule type="fa" cat="N/N">
      <lf start="0" span="1" word="Just" lemma="just" pos="RB" chunk="I-NP" entity="O" cat="(N/N)/(N/N)" />
      <lf start="1" span="1" word="one" lemma="one" pos="CD" chunk="I-NP" entity="O" cat="N/N" />
     </rule>
     <lf start="2" span="1" word="accountant" lemma="accountant" pos="NN" chunk="I-NP" entity="O" cat="N" />
    </rule>
   </rule>
   <rule type="fa" cat="S[dcl]\NP">
    <lf start="3" span="1" word="attended" lemma="attend" pos="VBD" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/NP" />
    <rule type="fa" cat="NP[nb]">
     <lf start="4" span="1" word="the" lemma="the" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
     <lf start="5" span="1" word="meeting" lemma="meeting" pos="NN" chunk="I-NP" entity="O" cat="N" />
    </rule>
   </rule>
  </rule>
  <lf start="6" span="1" word="." lemma="." pos="." chunk="O" entity="O" cat="." />
 </rule>
</ccg>
<deps>
<![CDATA[
]]></deps>
<ccg>
 <rule type="rp" cat="S[dcl]">
  <rule type="ba" cat="S[dcl]">
   <rule type="fa" cat="NP[nb]">
    <lf start="0" span="1" word="Some" lemma="some" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
    <lf start="1" span="1" word="accountant" lemma="accountant" pos="NN" chunk="I-NP" entity="O" cat="N" />
   </rule>
   <rule type="fa" cat="S[dcl]\NP">
    <lf start="2" span="1" word="attended" lemma="attend" pos="VBD" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/NP" />
    <rule type="fa" cat="NP[nb]">
     <lf start="3" span="1" word="the" lemma="the" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
     <lf start="4" span="1" word="meeting" lemma="meeting" pos="NN" chunk="I-NP" entity="O" cat="N" />
    </rule>
   </rule>
  </rule>
  <lf start="5" span="1" word="." lemma="." pos="." chunk="O" entity="O" cat="." />
 </rule>
</ccg>
<deps>
<![CDATA[
]]></deps>
</candc>
