<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="candc.xml"?>

<candc>
<ccg>
 <rule type="rp" cat="S[dcl]">
  <rule type="ba" cat="S[dcl]">
   <rule type="fa" cat="NP[nb]">
    <lf start="0" span="1" word="A" lemma="a" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
    <lf start="1" span="1" word="lawyer" lemma="lawyer" pos="NN" chunk="I-NP" entity="O" cat="N" />
   </rule>
   <rule type="ba" cat="S[dcl]\NP">
    <rule type="fa" cat="S[dcl]\NP">
     <lf start="2" span="1" word="signed" lemma="sign" pos="VBD" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/NP" />
     <rule type="fa" cat="NP[nb]">
      <lf start="3" span="1" word="every" lemma="every" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
      <lf start="4" span="1" word="report" lemma="report" pos="NN" chunk="I-NP" entity="O" cat="N" />
     </rule>
    </rule>
    <rule type="lp" cat="(S[dcl]\NP)\(S[dcl]\NP)">
     <lf start="5" span="1" word="," lemma="," pos="," chunk="O" entity="O" cat="," />
     <rule type="conj" cat="(S[dcl]\NP)\(S[dcl]\NP)">
      <lf start="6" span="1" word="and" lemma="and" pos="CC" chunk="O" entity="O" cat="conj" />
      <rule type="fa" cat="S[dcl]\NP">
       <lf start="7" span="1" word="so" lemma="so" pos="RB" chunk="I-VP" entity="O" cat="(S\NP)/(S\NP)" />
       <rule type="fa" cat="S[dcl]\NP">
        <lf start="8" span="1" word="did" lemma="do" pos="VBD" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/NP" />
        <rule type="fa" cat="NP[nb]">
         <lf start="9" span="1" word="an" lemma="an" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
         <lf start="10" span="1" word="auditor" lemma="auditor" pos="NN" chunk="I-NP" entity="O" cat="N" />
        </rule>
       </rule>
      </rule>
     </rule>
    </rule>
   </rule>
  </rule>
  <lf start="11" span="1" word="." lemma="." pos="." chunk="O" entity="O" cat="." />
 </rule>
</ccg>
<deps>
<![CDATA[
]]></deps>
<ccg>
 <rule type="rp" cat="S[dcl]">
  <rule type="ba" cat="S[dcl]">
   <lf start="0" span="1" word="That" lemma="that" pos="DT" chunk="I-NP" entity="O" cat="NP" />
   <rule type="fa" cat="S[dcl]\NP">
    <rule type="rp" cat="(S[dcl]\NP)/S[dcl]">
     <lf start="1" span="1" word="is" lemma="be" pos="VBZ" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/S[dcl]" />
     <lf start="2" span="1" word="," lemma="," pos="," chunk="O" entity="O" cat="," />
    </rule>
    <rule type="ba" cat="S[dcl]">
     <lf start="3" span="1" word="there" lemma="there" pos="EX" chunk="I-NP" entity="O" cat="NP[thr]" />
     <rule type="fa" cat="S[dcl]\NP[thr]">
      <lf start="4" span="1" word="was" lemma="be" pos="VBD" chunk="I-VP" entity="O" cat="(S[dcl]\NP[thr])/NP" />
      <rule type="ba" cat="NP">
       <rule type="lex" cat="NP">
        <rule type="fa" cat="N">
         <lf start="5" span="1" word="one" lemma="one" pos="CD" chunk="I-NP" entity="O" cat="N/N" />
         <lf start="6" span="1" word="lawyer" lemma="lawyer" pos="NN" chunk="I-NP" entity="O" cat="N" />
        </rule>
       </rule>
       <rule type="fa" cat="NP\NP">
        <lf start="7" span="1" word="who" lemma="who" pos="WP" chunk="B-NP" entity="O" cat="(NP\NP)/(S[dcl]\NP)" />
        <rule type="fa" cat="S[dcl]\NP">
         <lf start="8" span="1" word="signed" lemma="sign" pos="VBD" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/NP" />
         <rule type="fa" cat="NP">
          <lf start="9" span="1" word="all" lemma="all" pos="PDT" chunk="I-NP" entity="O" cat="NP/NP" />
          <rule type="fa" cat="NP[nb]">
           <lf start="10" span="1" word="the" lemma="the" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
           <lf start="11" span="1" word="reports" lemma="report" pos="NNS" chunk="I-NP" entity="O" cat="N" />
          </rule>
         </rule>
        </rule>
       </rule>
      </rule>
     </rule>
    </rule>
   </rule>
  </rule>
  <lf start="12" span="1" word="." lemma="." pos="." chunk="O" entity="O" cat="." />
 </rule>
</ccg>
<deps>
<![CDATA[
]]></deps>
<ccg>
 <rule type="rp" cat="S[dcl]">
  <rule type="ba" cat="S[dcl]">
   <lf start="0" span="1" word="There" lemma="there" pos="EX" chunk="I-NP" entity="O" cat="NP[thr]" />
   <rule type="fa" cat="S[dcl]\NP[thr]">
    <lf start="1" span="1" word="was" lemma="be" pos="VBD" chunk="I-VP" entity="O" cat="(S[dcl]\NP[thr])/NP" />
    <rule type="ba" cat="NP">
     <rule type="lex" cat="NP">
      <rule type="fa" cat="N">
       <lf start="2" span="1" word="one" lemma="one" pos="CD" chunk="I-NP" entity="O" cat="N/N" />
       <lf start="3" span="1" word="auditor" lemma="auditor" pos="NN" chunk="I-NP" entity="O" cat="N" />
      </rule>
     </rule>
     <rule type="fa" cat="NP\NP">
      <lf start="4" span="1" word="who" lemma="who" pos="WP" chunk="B-NP" entity="O" cat="(NP\NP)/(S[dcl]\NP)" />
      <rule type="fa" cat="S[dcl]\NP">
       <lf start="5" span="1" word="signed" lemma="sign" pos="VBD" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/NP" />
       <rule type="fa" cat="NP">
        <lf start="6" span="1" word="all" lemma="all" pos="PDT" chunk="I-NP" entity="O" cat="NP/NP" />
        <rule type="fa" cat="NP[nb]">
         <lf start="7" span="1" word="the" lemma="the" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
         <lf start="8" span="1" word="reports" lemma="report" pos="NNS" chunk="I-NP" entity="O" cat="N" />
        </rule>
       </rule>
      </rule>
     </rule>
    </rule>
   </rule>
  </rule>
  <lf start="9" span="1" word="." lemma="." pos="." chunk="O" entity="O" cat="." />
 </rule>
</ccg>
<deps>
<![CDATA[
]]></deps>
</candc>
