<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="candc.xml"?>

<candc>
<ccg>
 <rule type="rp" cat="S[dcl]">
  <rule type="ba" cat="S[dcl]">
   <rule type="fa" cat="NP[nb]">
    <lf start="0" span="1" word="Each" lemma="each" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
    <rule type="fa" cat="N">
     <lf start="1" span="1" word="Italian" lemma="italian" pos="JJ" chunk="I-NP" entity="O" cat="N/N" />
     <lf start="2" span="1" word="tenor" lemma="tenor" pos="NN" chunk="I-NP" entity="O" cat="N" />
    </rule>
   </rule>
   <rule type="fa" cat="S[dcl]\NP">
    <lf start="3" span="1" word="wants" lemma="want" pos="VBZ" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/(S[to]\NP)" />
    <rule type="fa" cat="S[to]\NP">
     <lf start="4" span="1" word="to" lemma="to" pos="TO" chunk="I-VP" entity="O" cat="(S[to]\NP)/(S[b]\NP)" />
     <rule type="fa" cat="S[b]\NP">
      <lf start="5" span="1" word="be" lemma="be" pos="VB" chunk="I-VP" entity="O" cat="(S[b]\NP)/(S[adj]\NP)" />
      <lf start="6" span="1" word="great" lemma="great" pos="JJ" chunk="I-ADJP" entity="O" cat="S[adj]\NP" />
     </rule>
    </rule>
   </rule>
  </rule>
  <lf start="7" span="1" word="." lemma="." pos="." chunk="O" entity="O" cat="." />
 </rule>
</ccg>
<deps>
<![CDATA[
]]></deps>
<ccg>
 <rule type="rp" cat="S[dcl]">
  <rule type="ba" cat="S[dcl]">
   <rule type="fa" cat="NP[nb]">
    <lf start="0" span="1" word="Some" lemma="some" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
    <rule type="fa" cat="N">
     <lf start="1" span="1" word="Italian" lemma="italian" pos="JJ" chunk="I-NP" entity="O" cat="N/N" />
     <lf start="2" span="1" word="tenors" lemma="tenor" pos="NNS" chunk="I-NP" entity="O" cat="N" />
    </rule>
   </rule>
   <rule type="fa" cat="S[dcl]\NP">
    <lf start="3" span="1" word="are" lemma="be" pos="VBP" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/(S[adj]\NP)" />
    <lf start="4" span="1" word="great" lemma="great" pos="JJ" chunk="I-ADJP" entity="O" cat="S[adj]\NP" />
   </rule>
  </rule>
  <lf start="5" span="1" word="." lemma="." pos="." chunk="O" entity="O" cat="." />
 </rule>
</ccg>
<deps>
<![CDATA[
]]></deps>
<ccg>
 <rule type="rp" cat="S[dcl]">
  <rule type="ba" cat="S[dcl]">
   <lf start="0" span="1" word="There" lemma="there" pos="EX" chunk="I-NP" entity="O" cat="NP[thr]" />
   <rule type="fa" cat="S[dcl]\NP[thr]">
    <lf start="1" span="1" word="are" lemma="be" pos="VBP" chunk="I-VP" entity="O" cat="(S[dcl]\NP[thr])/NP" />
    <rule type="ba" cat="NP">
     <rule type="lex" cat="NP">
      <rule type="fa" cat="N">
       <lf start="2" span="1" word="Italian" lemma="italian" pos="JJ" chunk="I-NP" entity="O" cat="N/N" />
       <lf start="3" span="1" word="tenors" lemma="tenor" pos="NNS" chunk="I-NP" entity="O" cat="N" />
      </rule>
     </rule>
     <rule type="fa" cat="NP\NP">
      <lf start="4" span="1" word="who" lemma="who" pos="WP" chunk="B-NP" entity="O" cat="(NP\NP)/(S[dcl]\NP)" />
      <rule type="fa" cat="S[dcl]\NP">
       <lf start="5" span="1" word="want" lemma="want" pos="VBP" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/(S[to]\NP)" />
       <rule type="fa" cat="S[to]\NP">
        <lf start="6" span="1" word="to" lemma="to" pos="TO" chunk="I-VP" entity="O" cat="(S[to]\NP)/(S[b]\NP)" />
        <rule type="fa" cat="S[b]\NP">
         <lf start="7" span="1" word="be" lemma="be" pos="VB" chunk="I-VP" entity="O" cat="(S[b]\NP)/(S[adj]\NP)" />
         <lf start="8" span="1" word="great" lemma="great" pos="JJ" chunk="I-ADJP" entity="O" cat="S[adj]\NP" />
        </rule>
       </rule>
      </rule>
     </rule>
    </rule>
   </rule>
  </rule>
  <lf start="9" span="1" word="." lemma="." pos="." chunk="O" entity="O" cat="." />
 </rule>
</ccg>
<deps>
<![CDATA[
]]></deps>
</candc>
