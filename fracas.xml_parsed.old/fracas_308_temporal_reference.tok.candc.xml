<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="candc.xml"?>

<candc>
<ccg>
 <rule type="rp" cat="S[dcl]">
  <rule type="ba" cat="S[dcl]">
   <rule type="lex" cat="NP">
    <lf start="0" span="1" word="Smith" lemma="Smith" pos="NNP" chunk="I-NP" entity="I-PER" cat="N" />
   </rule>
   <rule type="ba" cat="S[dcl]\NP">
    <rule type="fa" cat="S[dcl]\NP">
     <lf start="1" span="1" word="wrote" lemma="write" pos="VBD" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/PP" />
     <rule type="fa" cat="PP">
      <lf start="2" span="1" word="to" lemma="to" pos="TO" chunk="I-PP" entity="O" cat="PP/NP" />
      <rule type="fa" cat="NP[nb]">
       <lf start="3" span="1" word="a" lemma="a" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
       <lf start="4" span="1" word="representative" lemma="representative" pos="NN" chunk="I-NP" entity="O" cat="N" />
      </rule>
     </rule>
    </rule>
    <rule type="fa" cat="(S[X]\NP)\(S[X]\NP)">
     <lf start="5" span="1" word="every" lemma="every" pos="DT" chunk="B-NP" entity="I-DAT" cat="((S\NP)\(S\NP))/N" />
     <lf start="6" span="1" word="week" lemma="week" pos="NN" chunk="I-NP" entity="I-DAT" cat="N" />
    </rule>
   </rule>
  </rule>
  <lf start="7" span="1" word="." lemma="." pos="." chunk="O" entity="O" cat="." />
 </rule>
</ccg>
<deps>
<![CDATA[
]]></deps>
<ccg>
 <rule type="rp" cat="S[dcl]">
  <rule type="ba" cat="S[dcl]">
   <lf start="0" span="1" word="There" lemma="there" pos="EX" chunk="I-NP" entity="O" cat="NP[thr]" />
   <rule type="fa" cat="S[dcl]\NP[thr]">
    <lf start="1" span="1" word="is" lemma="be" pos="VBZ" chunk="I-VP" entity="O" cat="(S[dcl]\NP[thr])/NP" />
    <rule type="fa" cat="NP[nb]">
     <lf start="2" span="1" word="a" lemma="a" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
     <rule type="fa" cat="N">
      <lf start="3" span="1" word="representative" lemma="representative" pos="NN" chunk="I-NP" entity="O" cat="N/S[em]" />
      <rule type="fa" cat="S[em]">
       <lf start="4" span="1" word="that" lemma="that" pos="IN" chunk="I-SBAR" entity="O" cat="S[em]/S[dcl]" />
       <rule type="ba" cat="S[dcl]">
        <rule type="lex" cat="NP">
         <lf start="5" span="1" word="Smith" lemma="Smith" pos="NNP" chunk="I-NP" entity="I-PER" cat="N" />
        </rule>
        <rule type="fa" cat="S[dcl]\NP">
         <lf start="6" span="1" word="wrote" lemma="write" pos="VBD" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/PP" />
         <rule type="fa" cat="PP">
          <lf start="7" span="1" word="to" lemma="to" pos="TO" chunk="I-PP" entity="O" cat="PP/NP" />
          <rule type="fa" cat="NP[nb]">
           <lf start="8" span="1" word="every" lemma="every" pos="DT" chunk="I-NP" entity="I-DAT" cat="NP[nb]/N" />
           <lf start="9" span="1" word="week" lemma="week" pos="NN" chunk="I-NP" entity="I-DAT" cat="N" />
          </rule>
         </rule>
        </rule>
       </rule>
      </rule>
     </rule>
    </rule>
   </rule>
  </rule>
  <lf start="10" span="1" word="." lemma="." pos="." chunk="O" entity="O" cat="." />
 </rule>
</ccg>
<deps>
<![CDATA[
]]></deps>
</candc>
