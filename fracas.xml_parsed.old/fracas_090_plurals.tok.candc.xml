<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="candc.xml"?>

<candc>
<ccg>
 <rule type="rp" cat="S[dcl]">
  <rule type="ba" cat="S[dcl]">
   <rule type="fa" cat="NP[nb]">
    <lf start="0" span="1" word="The" lemma="the" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
    <lf start="1" span="1" word="chairman" lemma="chairman" pos="NN" chunk="I-NP" entity="O" cat="N" />
   </rule>
   <rule type="fa" cat="S[dcl]\NP">
    <rule type="bx" cat="(S[dcl]\NP)/NP">
     <lf start="2" span="1" word="read" lemma="read" pos="VBD" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/NP" />
     <lf start="3" span="1" word="out" lemma="out" pos="RP" chunk="I-PRT" entity="O" cat="(S\NP)\(S\NP)" />
    </rule>
    <rule type="ba" cat="NP">
     <rule type="fa" cat="NP[nb]">
      <lf start="4" span="1" word="the" lemma="the" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
      <lf start="5" span="1" word="items" lemma="item" pos="NNS" chunk="I-NP" entity="O" cat="N" />
     </rule>
     <rule type="fa" cat="NP\NP">
      <lf start="6" span="1" word="on" lemma="on" pos="IN" chunk="I-PP" entity="O" cat="(NP\NP)/NP" />
      <rule type="fa" cat="NP[nb]">
       <lf start="7" span="1" word="the" lemma="the" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
       <lf start="8" span="1" word="agenda" lemma="agenda" pos="NN" chunk="I-NP" entity="O" cat="N" />
      </rule>
     </rule>
    </rule>
   </rule>
  </rule>
  <lf start="9" span="1" word="." lemma="." pos="." chunk="O" entity="O" cat="." />
 </rule>
</ccg>
<deps>
<![CDATA[
]]></deps>
<ccg>
 <rule type="rp" cat="S[dcl]">
  <rule type="ba" cat="S[dcl]">
   <rule type="fa" cat="NP[nb]">
    <lf start="0" span="1" word="The" lemma="the" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
    <lf start="1" span="1" word="chairman" lemma="chairman" pos="NN" chunk="I-NP" entity="O" cat="N" />
   </rule>
   <rule type="ba" cat="S[dcl]\NP">
    <rule type="fa" cat="S[dcl]\NP">
     <rule type="bx" cat="(S[dcl]\NP)/NP">
      <lf start="2" span="1" word="read" lemma="read" pos="VBD" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/NP" />
      <lf start="3" span="1" word="out" lemma="out" pos="RP" chunk="I-PRT" entity="O" cat="(S\NP)\(S\NP)" />
     </rule>
     <rule type="fa" cat="NP[nb]">
      <lf start="4" span="1" word="every" lemma="every" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
      <lf start="5" span="1" word="item" lemma="item" pos="NN" chunk="I-NP" entity="O" cat="N" />
     </rule>
    </rule>
    <rule type="fa" cat="(S[X]\NP)\(S[X]\NP)">
     <lf start="6" span="1" word="on" lemma="on" pos="IN" chunk="I-PP" entity="O" cat="((S\NP)\(S\NP))/NP" />
