<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="candc.xml"?>

<candc>
<ccg>
 <rule type="rp" cat="S[dcl]">
  <rule type="ba" cat="S[dcl]">
   <rule type="fa" cat="NP[nb]">
    <lf start="0" span="1" word="A" lemma="a" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
    <rule type="fa" cat="N">
     <lf start="1" span="1" word="company" lemma="company" pos="NN" chunk="I-NP" entity="O" cat="N/N" />
     <lf start="2" span="1" word="director" lemma="director" pos="NN" chunk="I-NP" entity="O" cat="N" />
    </rule>
   </rule>
   <rule type="fa" cat="S[dcl]\NP">
    <rule type="fa" cat="(S[dcl]\NP)/NP">
     <lf start="3" span="1" word="awarded" lemma="award" pos="VBD" chunk="I-VP" entity="O" cat="((S[dcl]\NP)/NP)/NP" />
     <lf start="4" span="1" word="himself" lemma="himself" pos="PRP" chunk="I-NP" entity="O" cat="NP" />
    </rule>
    <rule type="fa" cat="NP[nb]">
     <lf start="5" span="1" word="a" lemma="a" pos="DT" chunk="B-NP" entity="O" cat="NP[nb]/N" />
     <rule type="fa" cat="N">
      <lf start="6" span="1" word="large" lemma="large" pos="JJ" chunk="I-NP" entity="O" cat="N/N" />
      <lf start="7" span="1" word="payrise" lemma="payrise" pos="NN" chunk="I-NP" entity="O" cat="N" />
     </rule>
    </rule>
   </rule>
  </rule>
  <lf start="8" span="1" word="." lemma="." pos="." chunk="O" entity="O" cat="." />
 </rule>
</ccg>
<deps>
<![CDATA[
]]></deps>
<ccg>
 <rule type="rp" cat="S[dcl]">
  <rule type="ba" cat="S[dcl]">
   <rule type="fa" cat="NP[nb]">
    <lf start="0" span="1" word="A" lemma="a" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
    <rule type="fa" cat="N">
     <lf start="1" span="1" word="company" lemma="company" pos="NN" chunk="I-NP" entity="O" cat="N/N" />
     <lf start="2" span="1" word="director" lemma="director" pos="NN" chunk="I-NP" entity="O" cat="N" />
    </rule>
   </rule>
   <rule type="fa" cat="S[dcl]\NP">
    <lf start="3" span="1" word="has" lemma="have" pos="VBZ" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/(S[pt]\NP)" />
    <rule type="ba" cat="S[pt]\NP">
     <lf start="4" span="1" word="awarded" lemma="award" pos="VBN" chunk="I-VP" entity="O" cat="S[pt]\NP" />
     <rule type="conj" cat="(S[pt]\NP)\(S[pt]\NP)">
      <lf start="5" span="1" word="and" lemma="and" pos="CC" chunk="I-VP" entity="O" cat="conj" />
      <rule type="fa" cat="S[pt]\NP">
       <lf start="6" span="1" word="been" lemma="be" pos="VBN" chunk="I-VP" entity="O" cat="(S[pt]\NP)/(S[pss]\NP)" />
       <rule type="fa" cat="S[pss]\NP">
        <lf start="7" span="1" word="awarded" lemma="award" pos="VBN" chunk="I-VP" entity="O" cat="(S[pss]\NP)/NP" />
        <rule type="fa" cat="NP[nb]">
         <lf start="8" span="1" word="a" lemma="a" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
         <lf start="9" span="1" word="payrise" lemma="payrise" pos="NN" chunk="I-NP" entity="O" cat="N" />
        </rule>
       </rule>
      </rule>
     </rule>
    </rule>
   </rule>
  </rule>
  <lf start="10" span="1" word="." lemma="." pos="." chunk="O" entity="O" cat="." />
 </rule>
</ccg>
<deps>
<![CDATA[
]]></deps>
</candc>
