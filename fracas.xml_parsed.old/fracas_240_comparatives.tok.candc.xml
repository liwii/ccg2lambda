<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="candc.xml"?>

<candc>
<ccg>
 <rule type="rp" cat="S[dcl]">
  <rule type="ba" cat="S[dcl]">
   <rule type="lex" cat="NP">
    <lf start="0" span="1" word="ITEL" lemma="ITEL" pos="NNP" chunk="I-NP" entity="I-ORG" cat="N" />
   </rule>
   <rule type="fa" cat="S[dcl]\NP">
    <lf start="1" span="1" word="won" lemma="win" pos="VBD" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/NP" />
    <rule type="ba" cat="NP">
     <rule type="lex" cat="NP">
      <rule type="fa" cat="N">
       <lf start="2" span="1" word="more" lemma="more" pos="JJR" chunk="I-NP" entity="O" cat="N/N" />
       <lf start="3" span="1" word="orders" lemma="order" pos="NNS" chunk="I-NP" entity="O" cat="N" />
      </rule>
     </rule>
     <rule type="fa" cat="NP\NP">
      <lf start="4" span="1" word="than" lemma="than" pos="IN" chunk="I-SBAR" entity="O" cat="(NP\NP)/S[dcl]" />
      <rule type="ba" cat="S[dcl]">
       <rule type="lex" cat="NP">
        <lf start="5" span="1" word="APCOM" lemma="APCOM" pos="NNP" chunk="I-NP" entity="I-ORG" cat="N" />
       </rule>
       <lf start="6" span="1" word="lost" lemma="lose" pos="VBD" chunk="I-VP" entity="O" cat="S[dcl]\NP" />
      </rule>
     </rule>
    </rule>
   </rule>
  </rule>
  <lf start="7" span="1" word="." lemma="." pos="." chunk="O" entity="O" cat="." />
 </rule>
</ccg>
<deps>
<![CDATA[
]]></deps>
<ccg>
 <rule type="rp" cat="S[dcl]">
  <rule type="ba" cat="S[dcl]">
   <rule type="lex" cat="NP">
    <lf start="0" span="1" word="APCOM" lemma="APCOM" pos="NNP" chunk="I-NP" entity="I-ORG" cat="N" />
   </rule>
   <rule type="fa" cat="S[dcl]\NP">
    <lf start="1" span="1" word="lost" lemma="lose" pos="VBD" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/NP" />
    <rule type="fa" cat="NP[nb]">
     <lf start="2" span="1" word="some" lemma="some" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
     <lf start="3" span="1" word="orders" lemma="order" pos="NNS" chunk="I-NP" entity="O" cat="N" />
    </rule>
   </rule>
  </rule>
  <lf start="4" span="1" word="." lemma="." pos="." chunk="O" entity="O" cat="." />
 </rule>
</ccg>
<deps>
<![CDATA[
]]></deps>
</candc>
