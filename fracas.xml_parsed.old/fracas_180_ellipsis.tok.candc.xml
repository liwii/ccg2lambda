<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="candc.xml"?>

<candc>
<ccg>
 <rule type="rp" cat="S[dcl]">
  <rule type="ba" cat="S[dcl]">
   <rule type="ba" cat="S[dcl]">
    <rule type="lex" cat="NP">
     <lf start="0" span="1" word="John" lemma="John" pos="NNP" chunk="I-NP" entity="I-PER" cat="N" />
    </rule>
    <rule type="fa" cat="S[dcl]\NP">
     <lf start="1" span="1" word="wanted" lemma="want" pos="VBD" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/(S[to]\NP)" />
     <rule type="fa" cat="S[to]\NP">
      <lf start="2" span="1" word="to" lemma="to" pos="TO" chunk="I-VP" entity="O" cat="(S[to]\NP)/(S[b]\NP)" />
      <rule type="fa" cat="S[b]\NP">
       <lf start="3" span="1" word="buy" lemma="buy" pos="VB" chunk="I-VP" entity="O" cat="(S[b]\NP)/NP" />
       <rule type="fa" cat="NP[nb]">
        <lf start="4" span="1" word="a" lemma="a" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
        <lf start="5" span="1" word="car" lemma="car" pos="NN" chunk="I-NP" entity="O" cat="N" />
       </rule>
      </rule>
     </rule>
    </rule>
   </rule>
   <rule type="lp" cat="S[dcl]\S[dcl]">
    <lf start="6" span="1" word="," lemma="," pos="," chunk="O" entity="O" cat="," />
    <rule type="conj" cat="S[dcl]\S[dcl]">
     <lf start="7" span="1" word="and" lemma="and" pos="CC" chunk="O" entity="O" cat="conj" />
     <rule type="ba" cat="S[dcl]">
      <lf start="8" span="1" word="he" lemma="he" pos="PRP" chunk="I-NP" entity="O" cat="NP" />
      <lf start="9" span="1" word="did" lemma="do" pos="VBD" chunk="I-VP" entity="O" cat="S[dcl]\NP" />
     </rule>
    </rule>
   </rule>
  </rule>
  <lf start="10" span="1" word="." lemma="." pos="." chunk="O" entity="O" cat="." />
 </rule>
</ccg>
<deps>
<![CDATA[
]]></deps>
<ccg>
 <rule type="rp" cat="S[dcl]">
  <rule type="ba" cat="S[dcl]">
   <rule type="lex" cat="NP">
    <lf start="0" span="1" word="John" lemma="John" pos="NNP" chunk="I-NP" entity="I-PER" cat="N" />
   </rule>
   <rule type="fa" cat="S[dcl]\NP">
    <lf start="1" span="1" word="bought" lemma="buy" pos="VBD" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/NP" />
    <rule type="fa" cat="NP[nb]">
     <lf start="2" span="1" word="a" lemma="a" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
     <lf start="3" span="1" word="car" lemma="car" pos="NN" chunk="I-NP" entity="O" cat="N" />
    </rule>
   </rule>
  </rule>
  <lf start="4" span="1" word="." lemma="." pos="." chunk="O" entity="O" cat="." />
 </rule>
</ccg>
<deps>
<![CDATA[
]]></deps>
</candc>
